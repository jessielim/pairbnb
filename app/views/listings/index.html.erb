 <% if @listing.blank? %>
  <h4>There are no listings containing the term <%= params[:search] %>.</h4>
<% end %>

<h1>All listings</h1>

<%= link_to "Create new listing", "/listings/new"%>
<table>

	<th>Listing ID</th>
	<th>Listing name</th>
	<th>Property Type</th>
	<th>Room Number</th>
	<th>Bed Number</th>
	<th>Guest Number</th>
	<th>Country</th>
	<th>State</th>
	<th>City</th>
	<th>Zipcode</th>
	<th>Address</th>
	<th>Price</th>
	<th>Description</th>
	<th>User ID</th>


<% @listing.each do |list| %>
	
	<tr>
	<td><%= list.id%></td>
	<td><%= list.name%></td>
	<td><%= list.property_type%></td>
	<td><%= list.room_number%></td>
	<td><%= list.bed_number%></td>
	<td><%= list.guest_number%></td>
	<td><%= list.country%></td>
	<td><%= list.state%></td>
	<td><%= list.city%></td>
	<td><%= list.zipcode%></td>
	<td><%= list.address%></td>
	<td><%= list.price%></td>
	<td><%= list.description%></td>
	<td><%= list.user_id%></td>
	<td><%= link_to "Edit", "/listings/#{list.id}/edit" %></td>
	<!-- <td><%= link_to "Verify" %></td> -->
	</tr>
<% end %>
	
</table>

<%= link_to "home", home_path %>